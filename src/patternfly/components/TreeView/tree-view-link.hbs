{{#> tree-view-node}}
  <{{# if tree-view-link--type}}{{tree-view-link--type}}{{else}}button{{/if}} class="pf-c-tree-view__link{{#if tree-view-link--modifier}} {{tree-view-link--modifier}}{{/if}}{{#if tree-view-link--IsFirst}} pf-m-first{{/if}}"
    role="treeitem"
    {{#if tree-view-link--IsFirst}}
      tabindex="0"
    {{else}}
      tabindex="-1"
    {{/if}}
    {{#if tree-view-link--attribute}}
      {{{tree-view-link--attribute}}}
    {{/if}}>
    {{#if tree-view-link--IsToggle}}
      {{> tree-view-link-toggle}}
    {{/if}}
    {{#if tree-view-link--HasCheckbox}}
      {{> tree-view-link-check}}
    {{/if}}
    {{#if tree-view-link--HasFolderIcon}}
      {{> tree-view-link-icon tree-view-link-icon--IsFolder="true"}}
    {{/if}}
    {{#if tree-view-link--text}}
      {{#> tree-view-text}}
        {{{tree-view-link--text}}}
      {{/tree-view-text}}
    {{/if}}
    {{#if @partial-block}}
      {{> @partial-block}}
    {{/if}}
  </{{# if tree-view-link--type}}{{tree-view-link--type}}{{else}}button{{/if}}>
{{/tree-view-node}}
