<span class="pf-c-context-menu__item-action-icon{{#if context-menu-item-action-icon--modifier}} {{context-menu-item-action-icon--modifier}}{{/if}}"
  {{#if context-menu-item-action-icon--attribute}}
    {{{context-menu-item-action-icon--attribute}}}
  {{/if}}>
  {{#if context-menu-item-action--IsFavorite}}
    <i class="fas fa-star" aria-label="Not starred"></i>
  {{else if context-menu-item-action--IsFavorited}}
    <i class="fas fa-star" aria-label="Starred"></i>
  {{else}}
    {{> @partial-block}}
  {{/if}}
</span>
